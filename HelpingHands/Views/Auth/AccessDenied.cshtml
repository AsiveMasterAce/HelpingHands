
@{
    ViewData["Title"] = "Access Denied";
}

<div class="container">

    <div class="row">  
    <div class="col-md-12 text-center">  
        <div class="top-buffer"></div>  
        <div class="panel panel-danger">  
            <div class="panel-heading">Access Denied</div>  
            <div class="panel-body">  
                <section>  
                    <h1 class="text-danger">403 ! Access Denied</h1>  
                    <h2 class="text-danger"> You not authorized for this</h2>
                    <br />  
                    <a href="javascript:void(0)" onClick="backAway()" class="btn btn-success">Back</a>  
                </section>  
            </div>  
        </div>  
    </div>  
    <div class="col-md-3"></div>  
</div>  
  
@section scripts  
{  
    <script>  
        function backAway() {  
            if (history.length === 1) {  
                window.location = "Home"  
            } else {  
                history.back();  
            }  
        }  
    </script>  
}   

</div>
